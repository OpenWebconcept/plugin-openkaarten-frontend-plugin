{
  "name": "acato/openkaarten-frontend-plugin",
  "type": "wordpress-plugin",
  "description": "Adds OpenKaarten Blocks with an Openstreetmap block to the Gutenberg editor.",
  "repositories": [
	{
	  "type": "git",
	  "url": "git@bitbucket.org:acato/wp-pre-commit-hook.git"
	}
  ],
  "require": {
    "composer/installers" : "~1.0"
  },
  "require-dev": {
	"acato/wp-pre-commit-hook": "*",
    "bjornjohansen/wp-pre-commit-hook" : "*",
    "phpcompatibility/phpcompatibility-wp": "*"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "acato/wp-pre-commit-hook": true
    }
  }
}
